{
    "Name": "fvnkhead.killstat",
    "Description": "Gather kill statistics and sends them to Tone API server.",
    "Version": "3.0.2",
    "LoadPriority": 2,
    "RequiredOnClient": false,
    "ConVars": [
        {
            "Name": "killstat_version",
            "DefaultValue": "ks_3.0.1"
        },
        {
            "Name": "Tone_URI",
            "DefaultValue": "https://tone.sleepycat.date/v3_test/server"
        },
        {
            "Name": "Tone_token",
            "DefaultValue": "YTZhMGRjYTctMDNjYi00Y2Y3LWJkYTktMTRlNTQxNDNhYjI2"
        },
        {
            "Name": "killstat_custom_parameters",
            "DefaultValue": ""
        }
    ],
    "Scripts": [
        {
            "Path": "killstat_shared.nut",
            "RunOn": "SERVER"
        },
        {
            "Path": "matchstat.nut",
            "RunOn": "SERVER",
            "ServerCallback": {
                "After": "matchStat_Init"
            }
        },
        {
            "Path": "killstat.nut",
            "RunOn": "SERVER",
            "ServerCallback": {
                "After": "killstat_Init"
            }
        }
    ]
}
